{
  "name": "data-admin",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite --open",
    "prepare": "husky install",
    "preview": "vite preview",
    "preview:test": "start-server-and-test preview http://localhost:4173",
    "commit": "cz",
    "format": "prettier -uw --ignore-path .gitignore .",
    "run-tsc": "tsc",
    "run-eslint": "eslint --cache --fix --ignore-path .gitignore --ext .ts,.tsx src",
    "run-stylelint": "stylelint --cache --fix --ignore-path .gitignore '**/*.css'",
    "lint": "run-p run-tsc run-eslint run-stylelint",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "antd": "^4.18.8",
    "axios": "^0.26.0",
    "clsx": "^1.2.0",
    "lodash": "^4.17.21",
    "qs": "^6.10.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-query": "^3.34.16",
    "react-router-dom": "^6.2.1",
    "styled-components": "^5.3.3",
    "three": "^0.138.3",
    "vite-react-jsx": "^1.1.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@commitlint/config-conventional": "^16.2.1",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-vite": "^0.1.38",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@types/babel__core": "^7.1.18",
    "@types/bmapgl": "^0.0.5",
    "@types/node": "^17.0.21",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/styled-components": "^5.1.23",
    "@types/three": "^0.138.0",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vitejs/plugin-legacy": "^1.7.1",
    "@vitejs/plugin-react": "^1.0.7",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "babel-loader": "^8.2.5",
    "commitizen": "^4.2.4",
    "commitlint": "^16.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-storybook": "^0.5.13",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vite-aliases": "^0.8.7",
    "vite-plugin-imp": "^2.1.6",
    "vite-plugin-inspect": "^0.3.15",
    "vite-plugin-svgr": "^1.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "defaultScope": "global"
    }
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "prettier --write",
      "git add"
    ]
  },
  "packageManager": "yarn@3.1.1"
}
